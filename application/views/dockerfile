  --label traefik.http.routers.mwandefinance.rule=Host\(\`mwandefinance.phamasoft.com\`\) \
  --label traefik.http.routers.mwandefinance.tls.certresolver=lets-encrypt \
  --label traefik.http.routers.mwandefinance.tls=true \
  --label traefik.http.routers.mwandefinance.service=mwandefinance-credit-http \
  --label traefik.http.services.mwandefinance-credit-http.loadbalancer.server.port=80 \
  --name mwandefinance \
  --network web \
  --restart always \
  --expose 80 \
