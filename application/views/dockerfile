  --label traefik.http.routers.kalidushi.service=kalidushi-credit-http \
  --label traefik.http.services.kalidushi-credit-http.loadbalancer.server.port=80 \
  --name kalidushi \
  --network web \
  --restart always \
  --expose 80 \
