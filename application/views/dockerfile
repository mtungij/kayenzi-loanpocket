docker run -it -d \
  --label traefik.http.routers.nackcredit.rule=Host\(\`nackcredit.com\`\) \
  --label traefik.http.routers.nackcredit.tls.certresolver=lets-encrypt \
  --label traefik.http.routers.nackcredit.tls=true \
  --label traefik.http.routers.nackcredit.service=nackcredit-credit-http \
  --label traefik.http.services.nackcredit-credit-http.loadbalancer.server.port=80 \
  --name nackcredit \
  --network web \
  --restart always \
  --expose 80 \
 emasanga/mikoposoft:may13 



