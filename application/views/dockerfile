docker run -it -d \
  --label traefik.http.routers.chrivemicrofinance.rule=Host\(\`chrivemicrofinance.com\`\) \
  --label traefik.http.routers.chrivemicrofinance.tls.certresolver=lets-encrypt \
  --label traefik.http.routers.chrivemicrofinance.tls=true \
  --label traefik.http.routers.chrivemicrofinance.service=chrivemicrofinance-credit-http \
  --label traefik.http.services.chrivemicrofinance-credit-http.loadbalancer.server.port=80 \
  --name chrivemicrofinance \
  --network web \
  --restart always \
  --expose 80 \
