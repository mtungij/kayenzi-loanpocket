docker run -it -d \
  --label traefik.http.routers.bamico.rule=Host\(\`bamico.phamasoft.com\`\) \
  --label traefik.http.routers.bamico.tls.certresolver=lets-encrypt \
  --label traefik.http.routers.bamico.tls=true \
  --label traefik.http.routers.bamico.service=bamico-credit-http \
  --label traefik.http.services.bamico-credit-http.loadbalancer.server.port=80 \
  --name bamico \
  --network web \
  --restart always \
  --expose 80 \
 emasanga/mikoposoft:may13 



